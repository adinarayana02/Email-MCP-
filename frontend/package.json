{
  "name": "ai-communication-assistant-frontend",
  "version": "1.0.0",
  "description": "Frontend for AI-Powered Communication Assistant - Email Management & Analytics Dashboard",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.0",
    "tailwindcss": "^3.3.6",
    "@headlessui/react": "^1.7.17",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "recharts": "^2.8.0",
    "axios": "^1.6.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "date-fns": "^2.30.0",
    "react-toastify": "^9.1.3",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "zustand": "^4.4.7",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "react-quill": "^2.0.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,css,md}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "proseWrap": "preserve"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-communication-assistant.git"
  },
  "keywords": [
    "ai",
    "communication",
    "email",
    "management",
    "dashboard",
    "analytics",
    "react",
    "tailwindcss",
    "fastapi"
  ],
  "author": "AI Communication Assistant Team",
  "license": "MIT"
}